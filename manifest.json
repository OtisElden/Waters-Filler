  "manifest_version": 3,
  "name": "Waters filler",
  "description": "Designed to auto fill temmplates and values for narritive",
  "version": "1.3.1",

  "icons": {

    "16": "white.png"
  },

  "action": {
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": [ "https://wyoming.imagetrendelite.com/*" ],
      "js": [ "contentScript.js" ]
    }
  ],

